export_dataset2 <- function(x, path = paste0(getwd(), "/", deparse(substitute(x))), type = "xlsx") {
#path <- gsub(".xlsx|.sav", path)
if(type == "spss") {
if(! grepl(".sav", path)) {
path <- paste0(path, ".sav")
}
write_sav(x, path)
} else {
if(! grepl(".xlsx", path)) {
path <- paste0(path, ".xlsx")
}
write_xlsx(x, path)
}
}
